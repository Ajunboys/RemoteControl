package com.foxconn.remote.control.utils;

/**
 * 轉發器通訊工具類
 * 
 * @author KrisLight
 */
public class IrUtil {
	public static boolean connnetToIrWithWifi(String irKey) {
		// 模擬Wifi連結
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		if ("123456".equals(irKey)) {
			return true;
		} else {
			return false;
		}
	}
	
	public static boolean connnetToIrWithBlueTooth(String irKey) {
		// 模擬藍牙連結
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		if ("123456".equals(irKey)) {
			return true;
		} else {
			return false;
		}
	}
}
